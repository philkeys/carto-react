{
  "compilerOptions": {
    "target": "es2020",
    "module": "es2020",
    "jsx": "react",
    "strict": true,
    "noImplicitAny": false,
    "allowJs": true,
    "checkJs": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@keys2design/carto-react-api/*": ["packages/react-api/src/*"],
      "@keys2design/carto-react-auth/*": ["packages/react-auth/src/*"],
      "@keys2design/carto-react-basemaps/*": ["packages/react-basemaps/src/*"],
      "@keys2design/carto-react-core/*": ["packages/react-core/src/*"],
      "@keys2design/carto-react-ui/*": ["packages/react-ui/src/*"],
      "@keys2design/carto-react-redux/*": ["packages/react-redux/src/*"],
      "@keys2design/carto-react-widgets/*": ["packages/react-widgets/src/*"],
      "@keys2design/carto-react-workers/*": ["packages/react-workers/src/*"]
    }
  }
}
